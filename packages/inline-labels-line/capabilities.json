  "dataViewMappings": [
    {
      "conditions": [
        {
          "xAxis": { "max": 2 },
          "group": { "max": 0 },
          "legend": { "max": 1 },
          "values": { "max": 1 }
        }
      ],
      "categorical": {
        "categories": {
          "select": [
            { "for": { "in": "xAxis" } },
            { "for": { "in": "legend" } }
          ],
          "dataReductionAlgorithm": {
            "top": { "count": 30000 }
          }
        },
        "values": {
          "select": [
            { "for": { "in": "values" } }
          ]
        }
      }
    },
    {
      "conditions": [
        {
          "xAxis": { "max": 2 },
          "group": { "min": 1, "max": 1 },
          "legend": { "max": 1 },
          "values": { "max": 1 }
        }
      ],
      "categorical": {
        "categories": {
          "select": [
            { "for": { "in": "xAxis" } },
            { "for": { "in": "legend" } }
          ],
          "dataReductionAlgorithm": {
            "top": { "count": 30000 }
          }
        },
        "values": {
          "group": {
            "by": "group",
            "select": [
              { "for": { "in": "values" } }
            ]
          }
        }
      }
    }
  ],
  "supportsHighlight": true,
  "supportsKeyboardFocus": true,
  "supportsLandingPage": true,
  "supportsEmptyDataView": true,
  "supportsMultiVisualSelection": true,
  "privileges": []
}
